{
  "name": "bnycdn",
  "description": "Simple cli for BunnyCDN service. This app is not an official one.",
  "version": "0.0.1",
  "author": "DKFN",
  "bin": {
    "bnycdn": "./bin/run"
  },
  "bugs": "https://github.com/DKFN/bnycdn/issues",
  "dependencies": {
    "@oclif/command": "^1.5.10",
    "@oclif/config": "^1.12.6",
    "@oclif/plugin-help": "^2.1.6",
    "axios": "^0.18.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.26",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/DKFN/bnycdn",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bnycdn",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "DKFN/bnycdn",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
